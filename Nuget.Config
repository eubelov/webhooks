<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="QuBit-Backend" value="https://nexus.qugo.ru/repository/nuget-hosted/" />
  </packageSources>
  <packageRestore>
    <add key="enabled" value="True" />
    <add key="automatic" value="True" />
  </packageRestore>
  <bindingRedirects>
    <add key="skip" value="False" />
  </bindingRedirects>
  <packageManagement>
    <add key="format" value="0" />
    <add key="disabled" value="False" />
  </packageManagement>
  <packageSourceCredentials>
    <QuBit-Backend>
        <add key="Username" value="nuget-admin" />
        <add key="ClearTextPassword" value="nuget-admin" />
    </QuBit-Backend>
  </packageSourceCredentials>
</configuration>